{
  "name": "valentine",
  "version": "1.0.0",
  "description": "Valentine MERN App",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"nodemon server.js\" \"npm run client\"",
    "client-install": "npm install --prefix client",
    "client": "npm start --prefix client",
    "build": "npm install && npm run client-install && npm run build --prefix client"
  },
